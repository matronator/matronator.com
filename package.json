{
  "name": "matronator.com",
  "version": "1.0.0",
  "description": "Source code for https://matronator.com",
  "source": "index.html",
  "browserslist": "IE 11",
  "scripts": {
    "dev": "npm run prebuild && npm-run-all -p start serve",
